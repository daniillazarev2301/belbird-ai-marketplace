# ===========================================
# Edge Functions Environment Variables
# Скопируйте в .env и заполните значениями
# ===========================================

# Supabase конфигурация (из вашего self-hosted Supabase)
SUPABASE_URL=http://localhost:8000
SUPABASE_ANON_KEY=ваш_anon_key_из_supabase
SUPABASE_SERVICE_ROLE_KEY=ваш_service_role_key_из_supabase

# AI API (замените на свой OpenAI/другой провайдер)
# Для AI функций нужен API ключ от провайдера
# Варианты: OpenAI, YandexGPT, GigaChat
LOVABLE_API_KEY=ваш_openai_api_key

# CDEK (опционально, для пунктов выдачи)
CDEK_CLIENT_ID=
CDEK_CLIENT_SECRET=
CDEK_TEST_MODE=true

# Boxberry (опционально, для пунктов выдачи)
BOXBERRY_TOKEN=

# Push уведомления (VAPID ключи)
# Сгенерировать: npx web-push generate-vapid-keys
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=
